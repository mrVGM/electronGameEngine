<div>
    <% var name = fileEntry.path.split('\\'); %>
    <% name = name[name.length - 1]; %>
    <div style="height: 15px;">
        <div><%= name %></div>
    </div>
    <% if (fileEntry.isDir && fileEntry.children.length > 0) { %>
        <div style="position: relative; left: 15px">
            <% for (var i = 0; i < fileEntry.children.length; ++i) { %>
                <%- api.visualizeFileEntry(fileEntry.children[i]) %>
            <% } %>
        </div>
    <% } %>
</div>